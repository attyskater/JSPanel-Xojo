		Sub child(_p as AttyWebJSPanel)
		  var DialogChild as  new AttyWebJSPanel
		  DialogChild.ParentContainer = _p
		  var refContchild as new Modalchild
		  DialogChild.Type = AttyWebJSPanel.types.Palette
		  DialogChild.DisableDragit= False
		  DialogChild.closeOnBackdrop = False
		  DialogChild.headerTitle = "Child"
		  DialogChild.headerLogo = "bi bi-bag-check"
		  DialogChild.MyPosition = AttyWebJSPanel.Position.center
		  DialogChild.AtPosition = AttyWebJSPanel.Position.center
		  DialogChild.ButtonMaximize = True
		  DialogChild.MinTop = 80
		  DialogChild.Resize = True
		  DialogChild.ButtonClose = False
		  DialogChild.ButtonMinimize = True
		  
		  if _p.Container <> nil AND _p.Container isA TestContainer then
		    DialogChild.minimizeTo = TestContainer(_p.Container).Rectangle1.ControlID
		  end if 
		  
		  AddHandler refContchild.Close, WeakAddressOf ModalData
		  refContchild.LockLeft = True
		  refContchild.LockTop = True
		  refContchild.EmbedWithin(self,0,0,refContchild.Width,refContchild.Height)
		  
		  DialogChild.Width = refContchild.Width
		  DialogChild.Height = refContchild.Height
		  DialogChild.Left = 0
		  
		  DialogChild.Height =refContchild.Height+34
		  AddControl(DialogChild)
		  DialogChild.Container = refContchild //trigger RecreateJsPanel
		  
		  
		  
		  
		  
		  
		End Sub
	#tag EndMethod

