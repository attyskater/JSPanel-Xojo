		Sub MasterSlave()
		  
		  var Dialog5 as new AttyWebJSPanel
		  var refTestContainer as new TestContainer
		  Dialog5.DisableDragit= False
		  Dialog5.closeOnBackdrop = False
		  Dialog5.headerTitle = "Palette Image"
		  Dialog5.headerLogo = "bi bi-bag-check"
		  Dialog5.MyPosition = AttyWebJSPanel.Position.center
		  Dialog5.AtPosition = AttyWebJSPanel.Position.center
		  Dialog5.ButtonMaximize = True
		  Dialog5.Resize = True
		  Dialog5.ButtonClose = True
		  Dialog5.MinTop = 80
		  
		  refTestContainer.LockLeft = True
		  refTestContainer.LockTop = True
		  refTestContainer.EmbedWithin(self,0,0,refTestContainer.Width,refTestContainer.Height)
		  Dialog5.Width = refTestContainer.Width
		  Dialog5.Height = refTestContainer.Height
		  Dialog5.Left = 0
		  
		  
		  var refDock() as AttyWebContainer
		  refDock.Add(new Modalchild)
		  refDock.Add(new Modalchild)
		  
		  for each rd as AttyWebContainer in refDock
		    rd.LockTop = True
		    rd.LockLeft = True
		    rd.EmbedWithin(self,0,0,rd.Width,rd.Height)
		  next
		  
		  Dialog5.DockContainer = refDock
		  
		  
		  AddControl(Dialog5)
		  Dialog5.Container = refTestContainer //trigger RecreateJsPanel
		  
		  
		  
		  
		End Sub
	#tag EndMethod

