		Sub Pressed()
		  
		  Var msgObj as new AttyWebYesOrNOMSG
		  msgObj.ID = "TestID"
		  msgObj.MSG = "Click button"
		  msgObj.fnDelegationResponse = WeakAddressOf RispostaYesOrNo
		  
		  
		  msgObj.Buttons.Append( new AttyWebAlertButton( "one", "button one", "bi bi-1-circle" ) )
		  msgObj.Buttons.Append( new AttyWebAlertButton( "two", "button two", "bi bi-2-circle" ) )
		  
		  MsgAlertAtty.ShowYesOrNo(msgObj, AttyWebJSPanel.MessageTypes.Info)
		End Sub
